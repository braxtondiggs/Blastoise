    "start": "nx run-many --target=serve --all",
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "e2e": "nx run-many --target=e2e --all",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "serve:web": "nx serve web",
    "serve:mobile": "nx serve mobile",
    "serve:api": "nx serve api",
    "dev": "nx run-many --target=serve --projects=api,web --parallel",
    "dev:mobile": "nx run-many --target=serve --projects=api,mobile --parallel",
    "graph": "nx graph",
    "migration:run": "npx tsx ./node_modules/typeorm/cli.js migration:run -d apps/api/src/database/migration.config.ts",
    "migration:revert": "npx tsx ./node_modules/typeorm/cli.js migration:revert -d apps/api/src/database/migration.config.ts",
    "migration:generate": "npx tsx ./node_modules/typeorm/cli.js migration:generate -d apps/api/src/database/migration.config.ts",
    "migration:show": "npx tsx ./node_modules/typeorm/cli.js migration:show -d apps/api/src/database/migration.config.ts",
    "migration:create": "npx tsx ./node_modules/typeorm/cli.js migration:create",
    "cap:sync": "nx sync mobile",
    "cap:open:ios": "nx open:ios mobile",
    "cap:open:android": "nx open:android mobile",
    "cap:run:ios": "nx run:ios mobile",
    "cap:run:android": "nx run:android mobile",
    "cap:assets": "npx capacitor-assets generate"
  },
