{
  "name": "beer",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build:prod && firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "serve:local": "firebase emulators:start",
    "analyze": "npm run build:prod && npx webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@angular/service-worker": "^15.2.10",
    "@ecodev/fab-speed-dial": "^12.0.0",
    "dayjs": "^1.10.5",
    "firebase": "^9.4.0",
    "humanize-duration-ts": "^2.1.1",
    "ngx-material-timepicker": "^5.5.3",
    "rxjs": "~7.5.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1502.11",
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular/cli": "^15.2.11",
    "@angular/compiler-cli": "^15.2.10",
    "@types/node": "^18.15.0",
    "firebase-tools": "^9.12.1",
    "typescript": "~4.9.5"
  }
}
